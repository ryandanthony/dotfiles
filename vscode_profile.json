{
    "editor.fontFamily": "'JetBrains Mono for Powerline', 'monospace', monospace, 'PowerlineSymbols'",
    "terminal.integrated.fontFamily": "'JetBrains Mono for Powerline', 'monospace', monospace, 'PowerlineSymbols'",
    "vs-kubernetes": {
        "vs-kubernetes.knownKubeconfigs": [
            "~/.kube/config"
        ],
        "vs-kubernetes.kubeconfig": "~/.kube/config"
    },
    "git.autofetch": true,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "workbench.colorTheme": "Visual Studio Dark"
}
